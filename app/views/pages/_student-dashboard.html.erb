
<div class="test">
  <div class="wrapper">
    <% current_user.requests.each do |request| %>
      <div class="student-card-show">
          <%= link_to image_tag(request.job.img_url), job_path(request.job), class: "card-img-top" %>

          <!-- A div with card__details class to hold the details in the card  -->
            <div class="student-card__details">

              <!-- Span with tag class for the tag -->
              <%# interrate over skills? @job.skills  %>
              <span class="tag">skills</span>

              <span class="tag">cooking</span>
              <!-- A div with name class for the name of the card -->
              <div class="name">
                <h4><%= request.job.job_title%></h4>
                <h4><%= request.job.job_description%></h4>
                  <p><strong>Start at:</strong><%= request.job.start_date%></p>
                  <p><strong>End:</strong><%= request.job.end_date%></p>
                <h5><%= request.job.remuneration%>â‚¬</h5>
              </div>
            </div>
            <a>
              <%= link_to "Cancel Application", request_path(request), class: "btn btn-primary", method: :delete, data: { confirm: "Are you sure?" } %>
            </a>
      </div>
    <% end %>
  </div>
